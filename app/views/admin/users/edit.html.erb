<h1>Editing user</h1>

<%= form_for @user, :as => :user,
  :url => admin_user_url(:id => @user),
  :html => {:multipart => true, :method => :put} do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= submit_tag 'Edit' %>
<% end %>

<%= link_to 'Show', admin_user_url(:id => @user) %> |
<%= link_to 'Back', admin_users_url %>
